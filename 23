#include <iostream>
using namespace std;
int main()
{
    int A, B;
    cout << "Please enter your numbers: " << endl;
    cin >> A >> B;

    cout << "If result is (0) it means false, or (1) it means true." << endl;

    cout << "Do " << A << " == " << B << " ===================== is " << (A == B) << endl;
    cout << "Do " << A << " != " << B << " ===================== is " << (A != B) << endl;
    cout << "Do " << A << " < " << B << " ===================== is " << (A < B) << endl;
    cout << "Do " << A << " > " << B << " ===================== is " << (A > B) << endl;
    cout << "Do " << A << " <= " << B << " ===================== is " << (A <= B) << endl;
    cout << "Do " << A << " >= " << B << " ===================== is " << (A >= B) << endl;

    return 0;
}
