#include <iostream>
#include <cmath>

using namespace std;
int main()
{
    
    int num;
    int pow1;
    
    cout << "please enter your number " << endl;
    cin >> num;
    cout << "and power " << endl;
    cin >> pow1;
    int R = pow(num, pow1);
    cout << "the result is " << round(R) << endl;

    return 0;
}
