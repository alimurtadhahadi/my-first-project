#include <iostream>
using namespace std;
int main()

{
    int num1;
  int num2;

  cout << "please enter two numbers" << endl;
  cin >> num1;
  cin >> num2;




  cout << endl;
  cout << "=====================================" << endl;
    cout << num1 + num2 << endl;
  cout << num1 - num2 << endl;
  cout << num1 * num2 << endl;
  cout << num1 / num2 << endl;
  cout << num1 % num2 << endl;

  return 0;




}
